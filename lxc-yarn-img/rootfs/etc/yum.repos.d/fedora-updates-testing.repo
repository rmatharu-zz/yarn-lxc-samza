[updates-testing]
name=Fedora $releasever - x86_64 - Test Updates
failovermethod=priority
#baseurl=http://download.fedoraproject.org/pub/fedora/linux/updates/testing/$releasever/x86_64/
metalink=https://mirrors.fedoraproject.org/metalink?repo=updates-testing-f$releasever&arch=x86_64
enabled=0
gpgcheck=1
metadata_expire=6h
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$releasever-x86_64
skip_if_unavailable=False

